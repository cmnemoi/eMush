{
   "name": "mush",
   "version": "1.0.0",
   "description": "This is a temporary repository for the Mush remake project, until it can get forked to the main ET repository.",
   "main": "build/src/index.js",
   "scripts": {
      "compile": "tsc",
      "prestart": "npm run compile",
      "start": "ts-node src/index.ts",
      "dev": "nodemon --watch src -e ts,ejs --exec npm run start",
      "pretest": "npm run compile && export NODE_ENV=test",
      "test": "export NODE_ENV=test && env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
      "linter-check": "gts check",
      "linter-fix": "gts fix",
      "generate_migration": "typeorm migration:generate -n migration",
      "run-migration": "typeorm migration:run"
   },
   "repository": {
      "type": "git",
      "url": "git+ssh://git@gitlab.com/eternal-twin/mush.git"
   },
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://gitlab.com/eternal-twin/mush/issues"
   },
   "engines": {
      "node": ">=14.0.0",
      "npm": ">=6.0.0"
   },
   "homepage": "https://gitlab.com/eternal-twin/mush#readme",
   "dependencies": {
      "body-parser": "^1.19.0",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-validator": "^6.6.1",
      "format-message": "^6.2.3",
      "jsonwebtoken": "^8.5.1",
      "lodash": "^4.17.20",
      "luxon": "^1.25.0",
      "mariadb": "^2.4.2",
      "mysql": "^2.14.1",
      "passport": "^0.4.1",
      "passport-custom": "^1.1.1",
      "passport-jwt": "^4.0.0",
      "swagger-ui-express": "^4.1.4",
      "reflect-metadata": "^0.1.10",
      "simple-oauth2": "^4.1.0",
      "typeorm": "0.2.26",
      "winston": "^3.3.3"
   },
   "devDependencies": {
      "@types/chai": "^4.2.12",
      "@types/express": "^4.17.8",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/lodash": "^4.14.161",
      "@types/luxon": "^1.25.0",
      "@types/mocha": "^8.0.3",
      "@types/node": "^8.0.29",
      "@types/passport": "^1.0.4",
      "@types/passport-jwt": "^3.0.3",
      "@types/passport-strategy": "^0.2.35",
      "@types/simple-oauth2": "^4.1.0",
      "@types/swagger-ui-express": "^4.1.2",
      "@types/validator": "^13.1.0",
      "@typescript-eslint/eslint-plugin": "^4.4.0",
      "@typescript-eslint/parser": "^4.4.0",
      "chai": "^4.2.0",
      "chai-http": "^4.3.0",
      "eslint": "^7.10.0",
      "gts": "^2.0.2",
      "mocha": "^8.1.3",
      "nodemon": "^2.0.4",
      "ts-node": "3.3.0",
      "tslint": "^6.1.3",
      "typescript": "^3.3.3333"
   }
}
