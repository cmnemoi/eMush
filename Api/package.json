  "description": "This is a temporary repository for the Mush remake project, until it can get forked to the main ET repository.",
  "main": "build/app.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "prestart": "npm run compile",
    "start": "node .",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run start",
    "test": "export NODE_ENV=test && env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/eternal-twin/mush.git"
  },
