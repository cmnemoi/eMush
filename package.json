{
  "name": "mush",
  "version": "1.0.0",
  "description": "This is a temporary repository for the Mush remake project, until it can get forked to the main ET repository.",
  "main": "dist/app.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run start",
    "test": "export NODE_ENV=test && env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/eternal-twin/mush.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/eternal-twin/mush/issues"
  },
  "homepage": "https://gitlab.com/eternal-twin/mush#readme",
  "dependencies": {
    "@types/validator": "^13.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mariadb": "^2.4.2",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/chai-http": "^4.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  }
}
